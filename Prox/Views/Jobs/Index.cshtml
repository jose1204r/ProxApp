


@model IEnumerable<Scheduele>




<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&display=swap">
    <link rel="stylesheet" href="css/tailwind/tailwind.min.css">
    <link rel="icon" type="image/png" sizes="32x32" href="shuffle-for-tailwind.png">
    <script src="js/main.js"></script>
    <title>Jobs</title>
</head>
<body>

    <h2>Jobs Scheduled</h2>



    <div class="row">

        <div class="col">
            <table id="tableschedule" class="table table-striped table-hover">


                <thead class="">

                    <tr>

                        <th>ID</th>
                        <th>NAME</th>
                        <th>DESCRIPTIONS</th>
                        <th>PROBLEMS</th>
                        <th>SCHEDULE DATE</th>
                        <th>PHONE</th>
                        <th>ADDRESS</th>
                        <th>City</th>
                        <th>STATE</th>
                        <th>ZIP CODE</th>
                        <th>EMAIL</th>
                    </tr>

                </thead>
                <tbody>


                    @foreach (var item in Model)

                    {


                        <tr>

                            <td><a href=/Jobs/ViewJob/@item.ID>@item.ID</a></td>

                           
                            <td>@item.Name</td>
                            <td>@item.Description</td>
                            <td>@item.Problems</td>
                            <td>@item.Datetime2</td>
                            <td>@item.Phone</td>
                            <td>@item.Address</td>
                            <td>@item.City</td>
                            <td>@item.State</td>
                            <td>@item.Zipcode</td>
                            <td>@item.Email</td>
                        </tr>





                    }



                </tbody>

            </table>


        </div>
        <a asp-controller="Jobs" asp-action="Schedule">Create</a>

    </div>












</body>
</html>